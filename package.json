{
  "name": "previte",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "check-format": "prettier --config .prettierrc -c .",
    "format": "prettier --config .prettierrc -w .",
    "dev:start": "vite",
    "prod:build": "rimraf build && npm run format && NODE_ENV=production vite build",
    "prod:build:serve": "npm run prod:build && vite preview"
  },
  "dependencies": {
    "preact": "^10.5.15"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.1.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "sass": "^1.49.9",
    "vite": "^2.8.0",
    "vite-plugin-pwa": "^0.11.13"
  }
}
